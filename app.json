{
  "expo": {
    "name": "connect",
    "slug": "connect",
    "owner": "dpkmht",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-sm.png",
    "splash": {
      // "image": "./assets/images/splash-new.png",
      "image": "./assets/images/splash-new-dark.png",
      "resizeMode": "cover"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 10000,
      "url": "https://u.expo.dev/f6ffa3f4-8805-43ca-a88e-a2297fbac759"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "connect.firebase",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Use the photo library to change your avatar."
      },
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "package": "connect.firebase",
      "versionCode": 1,
      "userInterfaceStyle": "automatic",
      "permissions": ["VIBRATE", "CAMERA"],
      "softwarekeyboardlayoutmode": "pan"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "svg", "ts", "tsx"]
    },
    "description": "",
    "extra": {
      "eas": {
        "projectId": "f6ffa3f4-8805-43ca-a88e-a2297fbac759"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
